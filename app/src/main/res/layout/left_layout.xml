<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <Button
        android:id="@+id/eq_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/eq_rds_layout_left"
        android:background="@drawable/eq_select" />

    <Button
        android:id="@+id/rds_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/eq_rds_layout_left"
        android:layout_marginTop="@dimen/rds_top"
        android:background="@drawable/rds_select"
        android:enabled="false"
        android:visibility="invisible" />

</LinearLayout>